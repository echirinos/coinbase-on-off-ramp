# This is an example environment file
# Copy this file to .env.local and fill in your own values

# OnchainKit API Key - Get this from Coinbase Developer Platform
NEXT_PUBLIC_ONCHAINKIT_API_KEY=

# OnchainKit Project Name
NEXT_PUBLIC_ONCHAINKIT_PROJECT_NAME=Coinbase Ramp Demo

# WalletConnect Project ID - Get this from WalletConnect Dashboard
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Coinbase Cloud Developer Platform API Key
NEXT_PUBLIC_COINBASE_CLOUD_API_KEY=

# Transaction Status API Endpoint
NEXT_PUBLIC_TRANSACTION_STATUS_API=https://api.developer.coinbase.com/onramp/v1/transactions

# CDP Project ID - Get this from Coinbase Developer Platform
NEXT_PUBLIC_CDP_PROJECT_ID=

# Server-side environment variables (not accessible in client components)
CDP_PROJECT_ID=
ONCHAINKIT_API_KEY=
WALLETCONNECT_PROJECT_ID=
